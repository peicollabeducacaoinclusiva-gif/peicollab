{
  "name": "@pei/pei-collab",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:integration": "node ../../scripts/health-check.js",
    "test:usability": "node ../../scripts/usability-test-runner.js",
    "generate:vapid": "node ../../scripts/generate-vapid-keys.js",
    "health:check": "node ../../scripts/health-check.js",
    "db:reset": "cd ../.. && supabase db reset --linked",
    "db:migrate": "cd ../.. && supabase db push",
    "db:diff": "cd ../.. && supabase db diff",
    "db:backup": "cd ../.. && supabase db dump --data-only > backup-$(date +%Y%m%d-%H%M%S).sql",
    "generate:peis-pdf": "node ../../scripts/gerar-peis-em-lote.js",
    "generate:all-peis-pdf": "node ../../scripts/gerar-peis-todos.js",
    "generate:sao-goncalo": "node ../../scripts/completar-peis-sao-goncalo.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
