{
  "name": "pei-collab",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:integration": "node scripts/health-check.js",
    "test:usability": "node scripts/usability-test-runner.js",
    "setup:production": "chmod +x scripts/setup-production.sh && ./scripts/setup-production.sh",
    "generate:vapid": "node scripts/generate-vapid-keys.js",
    "health:check": "node scripts/health-check.js",
    "db:reset": "supabase db reset --linked",
    "db:migrate": "supabase db push",
    "db:diff": "supabase db diff",
    "db:backup": "supabase db dump --data-only > backup-$(date +%Y%m%d-%H%M%S).sql",
    "performance:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "performance:monitor": "node scripts/performance-monitor.js",
    "notifications:test": "node scripts/test-push-notifications.js",
    "cache:clear": "node scripts/clear-cache.js",
    "cache:optimize": "node scripts/optimize-cache.js",
    "debug:database": "node scripts/database-tests.js",
    "debug:usability": "node scripts/usability-tests.js",
    "debug:performance": "node scripts/performance-tests.js",
    "fix:user-roles": "node scripts/fix-user-roles.js",
    "check:database": "node scripts/check-database-structure.js",
    "populate:user-roles": "node scripts/populate-user-roles.js",
    "refresh:schema": "node scripts/refresh-schema-cache.js",
    "quick:fix": "echo 'Execute o arquivo scripts/quick-fix-schema.sql no Supabase Dashboard'",
    "setup:env": "node scripts/setup-env.js",
    "fix:rls": "node scripts/fix-user-roles-rls.js",
    "test:profile": "node scripts/test-profile-loading.js",
    "fix:user-roles-final": "node scripts/apply-user-roles-fix.js",
    "disable:rls": "node scripts/disable-rls-final.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "postinstall": "npm run generate:vapid || echo 'VAPID keys generation failed - run manually'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^5.8.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "dexie": "^3.2.4",
    "lucide-react": "^0.294.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "react-sonner": "^1.2.4",
    "sonner": "^1.2.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.7.9",
    "web-push": "^3.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.55.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "globals": "^13.20.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "supabase": "^1.123.4",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/pei-collab.git"
  },
  "keywords": [
    "education",
    "pei",
    "special-education",
    "pwa",
    "offline-first",
    "mobile-first",
    "react",
    "typescript",
    "supabase"
  ],
  "author": "PEI Collab Team",
  "license": "MIT",
  "homepage": "https://pei-collab.com"
}